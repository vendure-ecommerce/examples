{
  "name": "vendure-templates-workspace",
  "version": "1.0.0",
  "description": "Vendure templates monorepo workspace",
  "private": true,
  "scripts": {
    "create-template": "node scripts/create-template.js",
    "dev:store": "pnpm --filter store dev",
    "build:store": "pnpm --filter store build",
    "dev:template": "pnpm --filter vendure-template-$1 dev",
    "build:template": "pnpm --filter vendure-template-$1 build",
    "install:all": "pnpm install",
    "clean": "pnpm -r clean",
    "list:templates": "ls -la templates/"
  },
  "keywords": ["vendure", "ecommerce", "templates", "monorepo"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "typescript": "5.8.2",
    "@types/node": "^20.0.0",
    "ts-node": "^10.0.0",
    "concurrently": "9.2.0",
    "@vendure/cli": "3.4.0"
  }
}