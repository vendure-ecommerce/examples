{
  "name": "playground-shop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:server": "ts-node ./src/index.ts",
    "dev:worker": "ts-node ./src/index-worker.ts",
    "dev": "concurrently npm:dev:*",
    "build": "tsc",
    "start:server": "node ./dist/index.js",
    "start:worker": "node ./dist/index-worker.js",
    "start": "concurrently npm:start:*",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "prepare": "husky",
    "export-csv": "npx ts-node ./src/scripts/export-low-stock-variants.ts"
  },
  "dependencies": {
    "@vendure/admin-ui-plugin": "3.4.1",
    "@vendure/asset-server-plugin": "3.4.1",
    "@vendure/core": "3.4.1",
    "@vendure/email-plugin": "3.4.1",
    "@vendure/graphiql-plugin": "3.4.1",
    "dotenv": "17.2.2",
    "pg": "8.16.3"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "@vendure/cli": "3.4.1",
    "concurrently": "9.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "typescript": "5.8.2"
  },
  "lint-staged": {
    "src/**/*.{ts,js,json}": [
      "prettier --write"
    ]
  }
}
