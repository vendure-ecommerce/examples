{
  "name": "s3-file-storage",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev:server": "node -r ts-node/register -r dotenv/config -r tsconfig-paths/register ./src/index.ts",
    "dev:worker": "node -r ts-node/register -r dotenv/config -r tsconfig-paths/register ./src/index-worker.ts",
    "dev": "concurrently npm:dev:*",
    "build": "tsc",
    "start:server": "node ./dist/index.js",
    "start:worker": "node ./dist/index-worker.js",
    "start": "concurrently npm:start:*"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.872.0",
    "@aws-sdk/lib-storage": "^3.872.0",
    "better-sqlite3": "11.10.0",
    "bcrypt": "5.1.1"
  },
  "description": "Platform-agnostic S3-compatible file storage for Vendure (AWS S3, Digital Ocean Spaces, MinIO, CloudFlare R2)",
  "keywords": [
    "vendure",
    "s3",
    "aws",
    "digital-ocean",
    "minio",
    "cloudflare-r2",
    "storage",
    "assets",
    "platform-agnostic",
    "example"
  ]
}