{
  "name": "vendure-examples-workspace",
  "version": "1.0.0",
  "description": "Vendure examples monorepo workspace",
  "private": true,
  "scripts": {
    "create-example": "node scripts/create-example.js",
    "dev:store": "pnpm --filter store dev",
    "build:store": "pnpm --filter store build",
    "dev:example": "pnpm --filter vendure-example-$1 dev",
    "build:example": "pnpm --filter vendure-example-$1 build",
    "install:all": "pnpm install",
    "clean": "pnpm -r clean",
    "list:examples": "ls -la examples/"
  },
  "keywords": ["vendure", "ecommerce", "examples", "monorepo"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "typescript": "5.8.2",
    "@types/node": "^20.0.0",
    "ts-node": "^10.0.0",
    "concurrently": "9.2.0",
    "@vendure/cli": "3.4.0"
  }
}