{
  "name": "shop-github-auth",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev:server": "node -r ts-node/register -r dotenv/config -r tsconfig-paths/register ./src/index.ts",
    "dev:worker": "node -r ts-node/register -r dotenv/config -r tsconfig-paths/register ./src/index-worker.ts",
    "dev": "concurrently npm:dev:*",
    "build": "tsc",
    "start:server": "node ./dist/index.js",
    "start:worker": "node ./dist/index-worker.js",
    "start": "concurrently npm:start:*"
  },
  "dependencies": {
    "better-sqlite3": "11.10.0",
    "bcrypt": "5.1.1"
  },
  "description": "Vendure example store: shop-github-auth",
  "keywords": [
    "example",
    "shop-github-auth"
  ]
}