{
  "name": "s3-file-storage",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev:server": "ts-node ./src/index.ts",
    "dev:worker": "ts-node ./src/index-worker.ts",
    "dev": "concurrently npm:dev:*",
    "build": "tsc",
    "start:server": "node ./dist/index.js",
    "start:worker": "node ./dist/index-worker.js",
    "start": "concurrently npm:start:*"
  },
  "dependencies": {
    "@vendure/admin-ui-plugin": "3.4.0",
    "@vendure/asset-server-plugin": "3.4.0",
    "@vendure/core": "3.4.0",
    "@vendure/email-plugin": "3.4.0",
    "@vendure/graphiql-plugin": "3.4.0",
    "@aws-sdk/client-s3": "^3.872.0",
    "@aws-sdk/lib-storage": "^3.872.0",
    "better-sqlite3": "11.10.0",
    "dotenv": "17.2.1",
    "bcrypt": "5.1.1"
  },
  "devDependencies": {
    "@vendure/cli": "3.4.0",
    "concurrently": "9.2.0",
    "typescript": "5.8.2"
  },
  "description": "Platform-agnostic S3-compatible file storage for Vendure (AWS S3, Digital Ocean Spaces, MinIO, CloudFlare R2)",
  "keywords": [
    "vendure",
    "s3",
    "aws",
    "digital-ocean",
    "minio",
    "cloudflare-r2",
    "storage",
    "assets",
    "platform-agnostic",
    "example"
  ]
}